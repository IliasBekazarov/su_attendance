#: venv/lib/python3.9/site-packages/django/contrib/flatpages/admin.py:13
msgid "Advanced options"
msgstr "Дагы мүмкүнчүлүктөр"

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/apps.py:8
msgid "Flat Pages"
msgstr "Туруктуу барактар"

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/forms.py:11
#: venv/lib/python3.9/site-packages/django/contrib/flatpages/models.py:9
msgid "URL"
msgstr "URL"

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/forms.py:15
msgid ""
"Example: “/about/contact/”. Make sure to have leading and trailing slashes."
msgstr ""
"Мисалы: “/about/contact/”. Башында жана аягында бөлчөк бар экендигин "
"текшериңиз."

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/forms.py:20
msgid ""
"This value must contain only letters, numbers, dots, underscores, dashes, "
"slashes or tildes."
msgstr ""
"Бул маани жалаң арип, сан, чекит, астсызык, сызык, бөлчөк же тилда камтый "
"алат."

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/forms.py:34
msgid "Example: “/about/contact”. Make sure to have a leading slash."
msgstr "Мисалы: “/about/contact”. Башында бөлчөк бар экендигин текшериңиз."

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/forms.py:47
msgid "URL is missing a leading slash."
msgstr "URL башында бөлчөгү жок."

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/forms.py:52
msgid "URL is missing a trailing slash."
msgstr "URL аягында бөлчөгү жок."

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/forms.py:69
#, python-format
msgid "Flatpage with url %(url)s already exists for site %(site)s"
msgstr "%(url)s URLдүү барак %(site)sсайтында мурдатан эле бар."

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/models.py:10
msgid "title"
msgstr "наам"

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/models.py:11
msgid "content"
msgstr "мазмун"

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/models.py:12
msgid "enable comments"
msgstr "пикирлерди күйгүзүү"

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/models.py:14
msgid "template name"
msgstr "калып аты"

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/models.py:18
msgid ""
"Example: “flatpages/contact_page.html”. If this isn’t provided, the system "
"will use “flatpages/default.html”."
msgstr ""
"Мисалы: “flatpages/contact_page.html”. Эгер бул көрсөтүлбөсө, систем "
"“flatpages/default.html” колдоно берет."

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/models.py:23
msgid "registration required"
msgstr "катталуу талап кылынат."

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/models.py:25
msgid "If this is checked, only logged-in users will be able to view the page."
msgstr ""
"Эгер бул тандалса жалаң гана катталып кирген колдонуучулар бул баракты көрө "
"алышат."

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/models.py:29
msgid "sites"
msgstr "сайттар"

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/models.py:33
msgid "flat page"
msgstr "туруктуу барак"

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/models.py:34
msgid "flat pages"
msgstr "туруктуу барактар"
